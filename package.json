{
  "name": "estudantinder-mobile",
  "main": "node_modules/expo/AppEntry.js",
  "version": "1.4.0",
  "scripts": {
    "ct": "git-cz",
    "start": "expo start",
    "deploy": "git push && expo publish --release-channel",
    "build:android": "git push && expo build:android --release-channel",
    "build:ios": "git push && expo build:ios --release-channel",
    "verify": "tsc --noEmit",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "jest": "cross-env NODE_ENV=test jest",
    "test:quiet": "cross-env NODE_ENV=test jest --notify",
    "test:ci": "cross-env NODE_ENV=test jest --coverage",
    "test:badges": "cross-env NODE_ENV=test jest --coverage && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__",
    "repo": "start msedge https://github.com/Cauaspinheiro/estudantinder-mobile"
  },
  "dependencies": {
    "@expo-google-fonts/archivo": "^0.1.0",
    "@expo-google-fonts/noto-sans": "^0.1.0",
    "@expo-google-fonts/poppins": "^0.1.0",
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-native-community/datetimepicker": "3.0.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-picker/picker": "^1.9.4",
    "@react-navigation/bottom-tabs": "^5.11.3",
    "@react-navigation/native": "^5.8.9",
    "@react-navigation/stack": "^5.12.6",
    "@unform/core": "^2.1.3",
    "@unform/mobile": "^2.1.3",
    "axios": "^0.21.1",
    "brazilian-values": "^0.10.1",
    "expo": "~39.0.2",
    "expo-constants": "~9.2.0",
    "expo-font": "~8.3.0",
    "expo-image-picker": "~9.1.1",
    "expo-status-bar": "~1.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.3.tar.gz",
    "react-native-appearance": "~0.3.3",
    "react-native-deck-swiper": "^2.0.5",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-masked-text": "^1.13.0",
    "react-native-pager-view": "^5.1.2",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "~1.13.0",
    "react-native-safe-area-context": "3.1.4",
    "react-native-screens": "~2.10.1",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-web": "~0.13.7",
    "rn-tooltip": "^3.0.1",
    "styled-components": "^5.2.1",
    "swr": "^0.4.0",
    "yup": "^0.31.0"
