{
  "compilerOptions": {
    "module": "esnext",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "target": "ES2017",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "strictFunctionTypes": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "baseUrl": ".",
    "paths": {
      "packages/*": [
        "./packages/*"
      ],
      "env": [
        "./env.ts"
      ],
      "App": [
        "./App.tsx"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
